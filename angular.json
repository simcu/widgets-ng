{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sim-widgets": {
      "projectType": "library",
      "root": "projects/sim-widgets",
      "sourceRoot": "projects/sim-widgets/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/sim-widgets/tsconfig.lib.json",
            "project": "projects/sim-widgets/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/sim-widgets/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/sim-widgets/src/test.ts",
            "tsConfig": "projects/sim-widgets/tsconfig.spec.json",
            "karmaConfig": "projects/sim-widgets/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/sim-widgets/tsconfig.lib.json",
              "projects/sim-widgets/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "inlineTemplate": true,
          "inlineStyle": true,
          "skipTests": true
        },
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "root": "projects/examples",
      "sourceRoot": "projects/examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/examples",
            "index": "projects/examples/src/index.html",
            "main": "projects/examples/src/main.ts",
            "polyfills": "projects/examples/src/polyfills.ts",
            "tsConfig": "projects/examples/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/examples/src/favicon.ico",
              "projects/examples/src/assets"
            ],
            "styles": [
              "projects/examples/src/styles.css",
              "node_modules/ng-zorro-antd/ng-zorro-antd.min.css",
              "node_modules/angular2-draggable/css/resizable.min.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/examples/src/environments/environment.ts",
                  "with": "projects/examples/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "examples:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "examples:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "examples:build"
          }
        }
      }
    }
  },
  "defaultProject": "sim-widgets"
}
